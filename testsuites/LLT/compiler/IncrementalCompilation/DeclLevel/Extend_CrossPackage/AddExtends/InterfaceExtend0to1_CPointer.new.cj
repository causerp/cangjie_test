package inc

public interface I1 {
    func foo() {
        1
    }
}

main() {
    let a = CPointer<UInt8>()
    a.foo()
}

extend<T> CPointer<T> <: I1 {
    public func foo() {
        if (isNull()) {
            0
        } else {
            1
        }
    }
}


