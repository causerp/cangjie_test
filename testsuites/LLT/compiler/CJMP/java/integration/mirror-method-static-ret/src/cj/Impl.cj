package cj

import interoplib.interop.*
import java.lang.JObject

@JavaImpl
public class Impl <: JObject {
    public init() {
        println("Impl()")

        let rcheck = MyObject.check()

        if (rcheck != 5) {
            throw Exception("MyObject.check() result is incorrect: ${rcheck}")
        }

        let rself = MyObject.self()
        let rvalue = rself.value()

        if (rvalue != 2) {
            throw Exception("MyObject.self().value() result is incorrect: ${rvalue}")
        }
    }
}