// ERRCHECK: %compiler %diag_json %f -o %output
// ASSERT: scan sema_inherit_member_type_inconsistent
// ASSERT: scan "Errors": 1,

open class C {
    public open func foo(): Int {
        1
    }
}

interface I {
    func foo(): String
}

func bar<T>(x: T) where T <: C & I {
    return x.foo()
}

class D <: C & I {
    public func foo(): Nothing {
        throw Exception()
    }
}

main() {
    try {
        bar<D>(D())
    } catch (_) {
        return 0
    }
    return 1
}
