import stdx.effect.Command

class Get <: Command<Int64> {}
class Eff <: Command<Unit> {}

main() {
    try {
        try {
            try {
                try {
                    perform Get()
                } handle (_: Eff) {
                    println("Never handled")
                }
            } catch (e: Exception) {
                println("Never caught by internal catch")
            }
            ()
        } handle (_: Get) {
            throw Exception("Hello")

            resume with 1
        }
    } catch (e: Exception) {
        println(e.message)
    }
    0
}
