import std.sync.*

let mtx: ReentrantMutex = ReentrantMutex()

func foo(){
  spawn{
    try {
        mtx.lock()
    } catch (_) {
        mtx.unlock()
    }
  }
  spawn{
    try {
        mtx.lock()
    } catch (_) {
        mtx.unlock()
    } finally {
    }
  }
  return 0
}

main(){
  return foo()
}

