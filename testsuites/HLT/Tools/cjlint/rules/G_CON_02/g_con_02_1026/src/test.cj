import std.sync.*

enum A{
  B
  func foo(){
    let mtx: ReentrantMutex = ReentrantMutex()
    spawn{
        try {
            mtx.lock()
        } catch (_) {
            mtx.unlock()
        }
        spawn{
            try {
                mtx.lock()
            } catch (_) {
            }
         }
    }
    return 0
  }
}

main(){
  return A.B.foo()
}

