struct Foo {
    let a = 0
    var b = 0
    const init() {}
    func f1() {}
    const func f2() {}
    mut func f3() {
        b = 123
    }
}

main(): Int64 {
    const v = Foo()
    print(v.a)
    print(v.b)
    v.f1()
    v.f2()
    return 0
}
