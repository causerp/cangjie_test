// LEVEL: 1
// EXEC: %compiler %cmp_opt %f -o %output
// RUN-EXEC: %run %run_opt %output %run_args

main(): Int64 {
    return run()
}

var x: Int64 = 2

func run(): Int64 {
    var res1 = Foo
    var res2 = res1(3)
    //    print(res2.toString()+"\n")
    if (res2 == 6) {
        return 0
    }
    return 1
}

func Foo(a!: Int64 = x): Int64 {
    return a * 2
}
