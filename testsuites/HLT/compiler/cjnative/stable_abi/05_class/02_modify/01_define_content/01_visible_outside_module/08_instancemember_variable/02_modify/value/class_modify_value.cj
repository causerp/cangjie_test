// test class: instancemember variable
@Annotation
public class Version {
    let code: String
    const init(code: String) {
        this.code = code
    }
}

public class CC {
    public let a1 = 10
    public var a2 = 20
    
    internal let b1 = 10
    internal var b2 = 2

    protected let c1 = 10
    protected var c2 = 2

    private let d1 = 10
    private var d2 = 2

    let e1 = 10
    var e2 = 20

    @Version["1.1"] public let f1 = 10
    @Version["1.1"] public var f2 = 20

    @Deprecated["This variable will be deprecated soon"] public let g1 = 10
    @Deprecated["This variable is deprecated", strict: true] public let g2 = 20

    let h1 = 10
