package test_stable_abi

// test class: instancemember variable

@Annotation
public class Version {
    let code: String
    const init(code: String) {
        this.code = code
    }
}

public class CC {
    static const a1 = 1
    static var a2 = 2
    static let a3 = 3

    @Version["1.1"] static const b1 = 1
    @Version["1.1"] static var b2 = 2
    @Version["1.1"] static let b3 = 3

    public static const func f1() {
        a1
    }

    @Frozen
    public func f2() {
        a2
    }

    @Frozen
    public func f3() {
        a3
    }

    public static const func f4() {
        b1
    }

    @Frozen
    public func f5() {
        b2
    }

    @Frozen
    public func f6() {
        b3
    }
}


