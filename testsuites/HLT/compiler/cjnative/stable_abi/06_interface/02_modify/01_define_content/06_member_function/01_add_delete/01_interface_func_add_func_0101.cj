package test_stable_abi

public func assert<T>(exp: T, act: T): Unit where T <: Equatable<T> & ToString {
    if (exp != act) { throw Exception("${exp} != ${act}") }
}
public interface I1 {
    static func g1() {1}
    func g2() {1}
    static func g3() {1}
}
public interface I2 {
    static func g1(): Int64
    func g2(): Int64
    static func g3() {1}
}
public class C1 <: I1 {}
public func f() {
    assert(C1.g1(), 1)
    assert(I1.g3(), 1)
    assert(I2.g3(), 1)
}
