package test_stable_abi

public func assert<T>(exp: T, act: T): Unit where T <: Equatable<T> & ToString {
    if (exp != act) { throw Exception("${exp} != ${act}") }
}
public interface I {
    static func g1() {1}
    func g2() {1}
    static const func g3() {2;1}
    const func g4() {2;1}
    @Frozen
    static func g5() {2;1}
    @Frozen
    func g6() {2;1}
}
public class C <: I {}
