// LEVEL: 1
// EXEC: %compiler %cmp_opt %f -o %output
// RUN-EXEC: %run %run_opt %output %run_args

class Test {
    public static var opt: Option<String> = Some<String>("option")
}

func run(): Int64 {
    var str: String = match (Test.opt) {
        case Some(x) => x
        case None => ""
    }
    if (str == "option") {
        return 0
    }
    return 1
}

main(): Int64 {
    return run()
}
