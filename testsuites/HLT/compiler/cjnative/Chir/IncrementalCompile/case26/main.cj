main(): Int64 {
    if (case01() != 11) {
        return 1
    }
    return 0
}

func case01(): Int64 {
    let intArray: Array<Int64> = [0, 1, 2, 3, 6, 11]
    var a: Int64 = 0
    for (item in intArray where item > 1) {
        a = item
    }
    a
}
