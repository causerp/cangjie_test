// LEVEL: 1
// DEPENDENCE: p1
// EXEC: %compiler %cmp_opt %compile_lib_opt ./p1/*.cj -o p1.%middle
// EXEC: %compiler %cmp_opt p1.%middle %f -o %output
// RUN-EXEC: %run %run_opt %output %run_args

import p1.A
import p1.B as Boo
import p1.C

main(): Int64 {
    if (A<Int32>.foo() != 1) {
        return 2
    }
    if (Boo<Int32>.foo<Int64>(1) != 1) {
        return 3
    }
    var a = C<Int32>.foo(Option<Int32>.None)
    //var a = C<Int32>.foo(None) // infer the type fail
    var r1 = match (a) {
        case Some(v) => v
        case None => Int32(1)
    }
    if (r1 != 1) {
        return 4
    }
    return 0
}
