// LEVEL: 0
// DEPENDENCE: ImportList_01.cj
// (CJNATIVE and not OHOS) EXEC: %compiler %import-cangjie-stdx %cmp_opt %f -o %output %cmp_utest_opt
// (CJNATIVE and not OHOS) RUN-EXEC: %run_stdx %output %run_utest_opt %run_args

import stdx.syntax.*

@Test
func test_ImportList_01() {
    let res = parseFile("./ImportList_01.cj")
    var diags = res.diags
    @Assert(diags.size, 0)

    let sourcefile: SourceFile = res.node.getOrThrow()
    sourcefile.toString()
    @Assert(sourcefile.toString().contains(##"public import a.*
import A.B.C.D.E.F.G.H.*
private import a as pkgA
internal import a.b.c.d.e.f.g as x1
protected import a.b.c.d.e.f.g as a_b_c_d_e_f_g
import {
    std.time,
}
protected import {
    std.time.*,
}
internal import {
    std.time as atime,
}
public import {
    std.time,
    std.fs as fileSystem,
    std.io.*,
}
private import std.{
    time,
    fs as fileSystem,
    io.*,
    collection.HashMap
}
protected import std.a.b.c.d.{
    time,
    fs as fileSystem,
    io.*,
    collection.HashMap
}
internal import {a, b.a, c.b.a, d.c.b.a as _d_c_b_a,}
import {a, b.a, c.b.a, d.c.b.a as _d_c_b_a}
private import a
protected import a.a.a.a.a.a.a.a.a.a.a.a.a
internal import a.b.c.d.e.f.g.h.i.j.k.l.m.n.o.p.q.r.s.t.u.v.w.x.y.z"##))
    @Assert(sourcefile.importLists.size, 16)

    var importLists1 = sourcefile.importLists[0]
    var pos = importLists1.getImportKeyWordPos()
    @Assert([pos.beginLine, pos.beginColumn, pos.endLine, pos.endColumn], [9i32, 8i32, 9i32, 14i32])
}
