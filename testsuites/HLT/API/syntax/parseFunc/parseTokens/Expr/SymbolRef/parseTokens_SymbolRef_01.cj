// (CJNATIVE and not OHOS) RUN-EXEC: %run_stdx %output %run_utest_opt %run_args

import stdx.syntax.*

@Test
func test_SymbolRef_01() {
    let _quote = quote(b)
    let res = parseTokens(_quote)
    let sourceNode: SyntaxTreeNode = res.node.getOrThrow()

    var _expr: SymbolRef = (sourceNode as SymbolRef).getOrThrow()
    @Expect(_expr.toString(), ##"b"##)
    @Assert(_expr.name, "b")
    @Assert(_expr.typeArguments.size, 0)
    @Assert(_expr.getCommasPos().size == 0, true)
    var pos = _expr.getIdentifierPos()
    @Assert([pos.beginLine, pos.beginColumn, pos.endLine, pos.endColumn], [1i32, 1i32, 1i32, 2i32])
    @Assert(_expr.getLAnglePos().isNone())
    @Assert(_expr.getRAnglePos().isNone())
}
