// LEVEL: 2
// DEPENDENCE: ../MyType
// (not MAC) EXEC: cd ./MyType && %compiler %cmp_opt --output-type=dylib --module-name=MyType ./src/myType/*.cj -o libMyType_myType.%test_suffix
// (not MAC) EXEC: %compiler %cmp_opt -o %output %f --import-path . -L ./MyType -lMyType_myType %cmp_utest_opt
// (not MAC) RUN-EXEC: %cp ./MyType/libMyType_myType.%test_suffix .&& %run %run_opt %output %run_args
import std.reflect.*
import MyType.myType.*

@Test
class Test {
    @TestCase
    func testInfoNotFoundException(): Unit {
        // 空字符串。
        try {
            TypeInfo.get("");
            @Assert(false)
        } catch (e: Exception) {
            @Expect(e.toString(), "InfoNotFoundException: TypeInfo '' not found.")
        }

        // 绝对不存在的类型名称。
        try {
            TypeInfo.get("xxx");
            @Assert(false)
        } catch (e: Exception) {
            @Expect(e.toString(), "InfoNotFoundException: TypeInfo 'xxx' not found.")
        }
    }
}
